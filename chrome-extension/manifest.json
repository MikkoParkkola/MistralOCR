{
  "manifest_version": 3,
  "name": "Mistral OCR Markdown Saver",
  "version": "1.0",
  "description": "Save page or selection to Markdown via Mistral OCR",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage",
    "contextMenus",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Save to Markdown"
  },
  "host_permissions": ["<all_urls>"]
}
